@model KriptoFeet.Users.Models.SingInData
@{
    ViewBag.Title = "SingIn";
}
<div class="row">
<div class="container col-md-4 col-md-offset-2">
<h2>Вход</h2>
<form action="/Home/SingIn" method="post" class="form-horizontal"> 
    <div class="sing-in-form">
        <div class="form-group">
        <label for="usr">Email:</label>
        <input type="text" name="Email" class="form-control" id="usr">
        </div>
        <div class="form-group">
        <label for="pwd">Password:</label>
        <input type="password" name="Password" class="form-control" id="pwd">
        </div>
    </div>
</form>
</div>
</div>