@model KriptoFeet.Users.Models.SignInData
@{
    ViewBag.Title = "SignIn";
}
<div class="row">
<div style="padding-left:0" class="col-md-4 col-md-offset-4">
    <h2>Вход</h2>
</div>
<div class="container col-md-4 col-md-offset-4">
<form asp-controller="Account" asp-action="SignIn" method="post" class="form-horizontal" asp-route-returnUrl="@Model.ReturnUrl"> 
    <div class="sing-in-form">
        <div class="form-group">
            <label asp-for="Nickname">Nickname:</label>
            <input type="text" class="form-control" asp-for="Nickname">
            <span class="validation-error" asp-validation-for="Nickname" />
        </div>
        <div class="form-group">
        <label asp-for="Password">Password:</label>
        <input type="password" class="form-control" asp-for="Password">
        </div>
         <div class="form-group"> 
       <div class="col-sm-offset-3 col-sm-9">
              <button asp-controller="Account" asp-action="SignIn" type="submit" class="btn btn-primary">Отправить</button>
     </div>
     <span class="validation-error" asp-validation-summary="All" />
   </div>
    </div>
</form>
</div>
</div>